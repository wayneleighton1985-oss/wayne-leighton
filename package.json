{
  "name": "wayne-leighton-books",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "tinacms dev -c \"astro dev\"",
    "build": "npx tinacms build && astro build && cp -r public/admin dist/",
    "build:full": "bash ./setup-env.sh && echo \"Starting TinaCMS build with client ID: $NEXT_PUBLIC_TINA_CLIENT_ID\" && tinacms build && astro build && cp -r public/admin dist/",
    "preview": "astro preview",
    "astro": "astro",
    "setup-env": "bash ./setup-env.sh",
    "setup-tina:dev": "bash ./setup-tina-dev.sh",
    "setup-tina:prod": "bash ./setup-tina-prod.sh",
    "deploy:prod": "bash ./deploy-production.sh"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.0",
    "@tinacms/auth": "^1.1.0",
    "@tinacms/cli": "^1.10.2",
    "astro": "^4.0.0",
    "next-auth": "^4.24.5",
    "tailwindcss": "^3.4.0",
    "tinacms": "^2.8.2",
    "tinacms-gitprovider-github": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.9.2"
  }
}
